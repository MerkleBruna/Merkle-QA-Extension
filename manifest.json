{
    "name": "Merkle QA Extension",
    "description": "QA extension to help with Target testing!",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["content.js"] 
        }
    ],
    "permissions": [
        "tabs"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "merkle_icon.png"
      }
  }